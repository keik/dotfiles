# editorconfig.org

root = true

[*]
indent_size = 2
indent_style = space
# end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{html,xml,css,less,js,json}]
indent_style = space
indent_size = 2

[*.py]
indent_style = space
indent_size = 4

[*.{java,jsp}]
indent_style = space
indent_size = 4
